1000 PNEXT=1
1010 DSKI$ 0,17,DSECT,D1$,D2$
1020 DI$=D1$+LEFT$(D2$,127)
1030 FOR ENTRY=LAST TO 7
1040 FLAG=ASC(MID$(DI$,ENTRY*32+1,1))
1050 IF FLAG=0 OR FLAG=255 THEN 1100
1060 IF MID$(DI$,ENTRY*32+9,3)<>"PIC" THEN 1100
1070 FOUND$(PNEXT)=MID$(DI$,ENTRY*32+1,8)
1080 PNEXT=PNEXT+1
1090 IF PNEXT=9 THEN 1150
1100 NEXT ENTRY
1110 LAST=0
1120 IF DSECT=11 THEN DSECT=3 ELSE DSECT=DSECT+1
1130 IF DSECT=3 THEN 1160
1140 GOTO 1010
1150 LAST=ENTRY+1
1160 RETURN
