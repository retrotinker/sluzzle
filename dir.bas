1000 FOUND=0
1010 FOR DSECT=3 TO 11
1020 DSKI$ DNUM,17,DSECT,D1$,D2$
1030 DI$=D1$+LEFT$(D2$,127)
1040 FOR ENTRY=0 TO 7
1050 FLAG=ASC(MID$(DI$,ENTRY*32+1,1))
1060 IF FLAG=0 OR FLAG=255 THEN 1100
1070 IF MID$(DI$,ENTRY*32+9,3)<>"SLZ" THEN 1100
1080 FOUND$(FOUND)=MID$(DI$,ENTRY*32+1,8)
1090 FOUND=FOUND+1
1100 NEXT ENTRY
1110 NEXT DSECT
1120 RETURN
