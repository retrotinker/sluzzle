1500 CLS
1510 PRINT @ 480, "LOADING "+IMAGE$+"...";
1520 LOADM IMAGE$+"/PIC"
1530 CLS
1540 RC=USR0(0)
1550 IF RC<=0 THEN 1580
1560 PRINT @ 448, "GAME WON AFTER"; RC; "MOVES!"
1570 GOTO 1590
1580 PRINT @ 448, "FORFEIT AFTER"; ABS(RC); "MOVES..."
1590 INPUT "PLAY AGAIN (Y/N)"; YN$
1600 IF YN$="N" THEN END
1610 IF YN$<>"Y" THEN 1590
1620 INPUT "SAME PICTURE (Y/N)"; YN$
1630 IF YN$="Y" THEN 1530
1640 IF YN$<>"N" THEN 1620
1650 RETURN
