1500 CLS
1510 PRINT @ 224, "LOADING "+IMAGE$+"..."
1520 LOADM IMAGE$+"/PIC"
1530 CLS
1540 RC=USR0(0)
1550 IF RC<=0 THEN 1580
1560 PRINT @ 192, "GAME WON AFTER"; RC; "MOVES!"
1570 GOTO 1590
1580 PRINT @ 192, "FORFEIT AFTER"; ABS(RC); "MOVES..."
1590 PRINT
1600 INPUT "PLAY AGAIN? (Y/N)"; YN$
1610 IF YN$="N" THEN END
1620 IF YN$<>"Y" THEN 1600
1630 INPUT "SAME IMAGE? (Y/N)"; YN$
1640 IF YN$="Y" THEN 1530
1650 IF YN$<>"N" THEN 1630
1660 RETURN
